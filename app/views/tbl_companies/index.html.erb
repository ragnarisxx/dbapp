<p id="notice"><%= notice %></p>

<h1>Listing Tbl Companies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tbl_companies.each do |tbl_company| %>
      <tr>
        <td><%= link_to 'Show', tbl_company %></td>
        <td><%= link_to 'Edit', edit_tbl_company_path(tbl_company) %></td>
        <td><%= link_to 'Destroy', tbl_company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tbl company', new_tbl_company_path %>
